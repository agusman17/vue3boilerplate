{"ast":null,"code":"import AssetForm from '../components/modules/form/AssetForm.vue';\nimport AssetList from '../components/modules/list/asset/AssetList.vue';\nimport { UseAssetStore } from '../store/assets';\nexport default {\n  name: \"Home\",\n  props: {\n    showAdd: Boolean\n  },\n  components: {\n    AssetForm,\n    AssetList,\n    UseAssetStore\n  },\n  data() {\n    return {\n      assets: [],\n      store: UseAssetStore()\n    };\n  },\n  methods: {\n    async deleteAsset(id) {\n      return await this.assets.filter(assets => assets.id !== id);\n    },\n    async addAsset(newAsset) {\n      this.assets = [...this.assets, newAsset];\n    },\n    async brokenAsset(id) {\n      filterBroken = await this.fetchAsset(id);\n      updBroken = {\n        ...filterBroken,\n        broken: !filterBroken.broken\n      };\n      this.assets = await this.assets.map(asset => asset.id === id ? {\n        ...asset,\n        broken: !asset.broken\n      } : asset);\n    },\n    async fetchAssets() {\n      await this.store.fetchAssets();\n      return this.store.getAssets;\n    },\n    async fetchAsset(id) {\n      return await this.store.fetchAsset(id);\n    }\n  },\n  async created() {\n    this.assets = await this.fetchAssets();\n  }\n};","map":{"version":3,"names":["AssetForm","AssetList","UseAssetStore","name","props","showAdd","Boolean","components","data","assets","store","methods","deleteAsset","id","filter","addAsset","newAsset","brokenAsset","filterBroken","fetchAsset","updBroken","broken","map","asset","fetchAssets","getAssets","created"],"sources":["/Users/agusmanriyadi/Documents/LATIHAN/VUE 3/vuepractice/src/views/Home.vue"],"sourcesContent":["<template>\n    \n    <AssetForm v-show=\"showAdd\" @action-add=\"addAsset\"/>\n\n    <div class=\"my-2\">\n        <AssetList :assets=\"assets\" @action-delete=\"deleteAsset\" @action-broken=\"brokenAsset\"/>\n    </div>\n\n</template>\n\n<script>\nimport AssetForm from '../components/modules/form/AssetForm.vue'\nimport AssetList from '../components/modules/list/asset/AssetList.vue'\nimport {UseAssetStore} from '../store/assets'\n\nexport default {\n    name: \"Home\",\n    props:{\n        showAdd: Boolean\n    },\n    components: {\n        AssetForm,\n        AssetList,\n        UseAssetStore\n    },\n    data(){\n        return {\n            assets : [],\n            store : UseAssetStore()\n        }\n    },\n    methods:{\n        async deleteAsset(id){\n            return await this.assets.filter((assets) => assets.id !== id)\n        },\n        async addAsset(newAsset){\n            this.assets = [...this.assets, newAsset]\n        },\n        async brokenAsset(id){\n\n            filterBroken = await this.fetchAsset(id)\n            updBroken = {...filterBroken, broken: !filterBroken.broken}\n\n            \n\n            this.assets = await this.assets.map( (asset) => asset.id === id ? {...asset, broken : !asset.broken} : asset)\n        },\n        async fetchAssets(){\n            await this.store.fetchAssets()\n            return this.store.getAssets \n        },\n        async fetchAsset(id){\n            return await this.store.fetchAsset(id)\n        }\n    },\n    async created(){\n        this.assets = await this.fetchAssets()\n    }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAWA,OAAOA,SAAQ,MAAO,0CAAyC;AAC/D,OAAOC,SAAQ,MAAO,gDAA+C;AACrE,SAAQC,aAAa,QAAO,iBAAgB;AAE5C,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,KAAK,EAAC;IACFC,OAAO,EAAEC;EACb,CAAC;EACDC,UAAU,EAAE;IACRP,SAAS;IACTC,SAAS;IACTC;EACJ,CAAC;EACDM,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,MAAK,EAAI,EAAE;MACXC,KAAI,EAAIR,aAAa,CAAC;IAC1B;EACJ,CAAC;EACDS,OAAO,EAAC;IACJ,MAAMC,WAAWA,CAACC,EAAE,EAAC;MACjB,OAAO,MAAM,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAEL,MAAM,IAAKA,MAAM,CAACI,EAAC,KAAMA,EAAE;IAChE,CAAC;IACD,MAAME,QAAQA,CAACC,QAAQ,EAAC;MACpB,IAAI,CAACP,MAAK,GAAI,CAAC,GAAG,IAAI,CAACA,MAAM,EAAEO,QAAQ;IAC3C,CAAC;IACD,MAAMC,WAAWA,CAACJ,EAAE,EAAC;MAEjBK,YAAW,GAAI,MAAM,IAAI,CAACC,UAAU,CAACN,EAAE;MACvCO,SAAQ,GAAI;QAAC,GAAGF,YAAY;QAAEG,MAAM,