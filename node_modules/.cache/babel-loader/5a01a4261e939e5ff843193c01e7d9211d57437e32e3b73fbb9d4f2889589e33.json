{"ast":null,"code":"import AssetForm from '../components/modules/form/AssetForm.vue';\nimport AssetList from '../components/modules/list/asset/AssetList.vue';\nimport { UseAssetStore } from '../store/assets';\nexport default {\n  name: \"Home\",\n  props: {\n    showAdd: Boolean\n  },\n  components: {\n    AssetForm,\n    AssetList,\n    UseAssetStore\n  },\n  data() {\n    return {\n      assets: [],\n      store: UseAssetStore()\n    };\n  },\n  methods: {\n    async deleteAsset(id) {\n      return await this.assets.filter(assets => assets.id !== id);\n    },\n    async addAsset(newAsset) {\n      this.assets = [...this.assets, newAsset];\n    },\n    async brokenAsset(id) {\n      console.log(id);\n      this.assets = await this.assets.map(asset => asset.id === id ? {\n        ...asset,\n        broken: !asset.broken\n      } : asset);\n    },\n    async fetchAssets() {\n      await this.store.fetchAssets();\n      return this.store.getAssets;\n    }\n  },\n  created() {\n    this.assets = this.fetchAssets();\n    console.log(this.assets);\n  }\n};","map":{"version":3,"names":["AssetForm","AssetList","UseAssetStore","name","props","showAdd","Boolean","components","data","assets","store","methods","deleteAsset","id","filter","addAsset","newAsset","brokenAsset","console","log","map","asset","broken","fetchAssets","getAssets","created"],"sources":["/Users/agusmanriyadi/Documents/LATIHAN/VUE 3/vuepractice/src/views/Home.vue"],"sourcesContent":["<template>\n    \n    <AssetForm v-show=\"showAdd\" @action-add=\"addAsset\"/>\n\n    <div class=\"my-2\">\n        <AssetList :assets=\"assets\" @action-delete=\"deleteAsset\" @action-broken=\"brokenAsset\"/>\n    </div>\n\n</template>\n\n<script>\nimport AssetForm from '../components/modules/form/AssetForm.vue'\nimport AssetList from '../components/modules/list/asset/AssetList.vue'\nimport {UseAssetStore} from '../store/assets'\n\nexport default {\n    name: \"Home\",\n    props:{\n        showAdd: Boolean\n    },\n    components: {\n        AssetForm,\n        AssetList,\n        UseAssetStore\n    },\n    data(){\n        return {\n            assets : [],\n            store : UseAssetStore()\n        }\n    },\n    methods:{\n        async deleteAsset(id){\n            return await this.assets.filter((assets) => assets.id !== id)\n        },\n        async addAsset(newAsset){\n            this.assets = [...this.assets, newAsset]\n        },\n        async brokenAsset(id){\n            console.log(id)\n            this.assets = await this.assets.map( (asset) => asset.id === id ? {...asset, broken : !asset.broken} : asset)\n        },\n        async fetchAssets(){\n            await this.store.fetchAssets()\n            return this.store.getAssets \n        }\n    },\n    created(){\n        this.assets = this.fetchAssets()\n        console.log(this.assets)\n    }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAWA,OAAOA,SAAQ,MAAO,0CAAyC;AAC/D,OAAOC,SAAQ,MAAO,gDAA+C;AACrE,SAAQC,aAAa,QAAO,iBAAgB;AAE5C,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,KAAK,EAAC;IACFC,OAAO,EAAEC;EACb,CAAC;EACDC,UAAU,EAAE;IACRP,SAAS;IACTC,SAAS;IACTC;EACJ,CAAC;EACDM,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,MAAK,EAAI,EAAE;MACXC,KAAI,EAAIR,aAAa,CAAC;IAC1B;EACJ,CAAC;EACDS,OAAO,EAAC;IACJ,MAAMC,WAAWA,CAACC,EAAE,EAAC;MACjB,OAAO,MAAM,IAAI,CAACJ,MAAM,CAACK,MAAM,CAAEL,MAAM,IAAKA,MAAM,CAACI,EAAC,KAAMA,EAAE;IAChE,CAAC;IACD,MAAME,QAAQA,CAACC,QAAQ,EAAC;MACpB,IAAI,CAACP,MAAK,GAAI,CAAC,GAAG,IAAI,CAACA,MAAM,EAAEO,QAAQ;IAC3C,CAAC;IACD,MAAMC,WAAWA,CAACJ,EAAE,EAAC;MACjBK,OAAO,CAACC,GAAG,CAACN,EAAE;MACd,IAAI,CAACJ,MAAK,GAAI,MAAM,IAAI,CAACA,MAAM,CAACW,GAAG,CAAGC,KAAK,IAAKA,KAAK,CAACR,EAAC,KAAMA,EAAC,GAAI;QAAC,GAAGQ,KAAK;QAAEC,MAAK,EAAI,CAACD,KAAK,CAACC;MAAM,IAAID,KAAK;IAChH,CAAC;IACD,MAAME,WAAWA,CAAA,EAAE;MACf,MAAM,IAAI,CAACb,KAAK,CAACa,WAAW,CAAC;MAC7B,OAAO,IAAI,CAACb,KAAK,CAACc,SAAQ;IAC9B;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACL,IAAI,CAAChB,MAAK,GAAI,IAAI,CAACc,WAAW,CAAC;IAC/BL,OAAO,CAACC,GAAG,CAAC,IAAI,CAACV,MAAM;EAC3B;AACJ"},"metadata":{},"sourceType