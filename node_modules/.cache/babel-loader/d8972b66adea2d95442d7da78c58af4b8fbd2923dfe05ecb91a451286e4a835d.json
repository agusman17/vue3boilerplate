{"ast":null,"code":"import AssetForm from '../components/modules/form/AssetForm.vue';\nimport AssetList from '../components/modules/list/asset/AssetList.vue';\nimport { UseAssetStore } from '../store/AssetStore';\nimport axios from \"axios\";\nexport default {\n  name: \"Home\",\n  props: {\n    showAdd: Boolean\n  },\n  components: {\n    AssetForm,\n    AssetList,\n    UseAssetStore\n  },\n  data() {\n    return {\n      assets: [],\n      store: UseAssetStore()\n    };\n  },\n  methods: {\n    async deleteAsset(id) {\n      if (confirm(\"Are you sure ?\")) {\n        const res = await this.store.deleteAsset(id);\n        res.status === 200 ? this.assets = this.assets.filter(asset => asset.id !== id) : alert(\"Error deleting asset\");\n      }\n    },\n    async addAsset(newAsset) {\n      const res = await this.store.addAsset(newAsset);\n      if (res.status === 201) {\n        alert(\"Data asset has been submit\");\n        this.assets = [...this.assets, res.data];\n      }\n    },\n    async brokenAsset(id) {\n      const filterBroken = await this.fetchAsset(id);\n      const updBroken = {\n        ...filterBroken,\n        broken: !filterBroken.broken\n      };\n      const res = this.res.updateAsset(id, updBroken);\n      const data = res.data;\n      this.assets = this.assets.map(asset => asset.id === id ? {\n        ...asset,\n        broken: data.broken\n      } : asset);\n    },\n    async fetchAssets() {\n      await this.store.fetchAssets();\n      return this.store.getAssets;\n    },\n    async fetchAsset(id) {\n      const data = await this.store.fetchAsset(id);\n      return data;\n    }\n  },\n  async created() {\n    this.assets = await this.fetchAssets();\n  }\n};","map":{"version":3,"names":["AssetForm","AssetList","UseAssetStore","axios","name","props","showAdd","Boolean","components","data","assets","store","methods","deleteAsset","id","confirm","res","status","filter","asset","alert","addAsset","newAsset","brokenAsset","filterBroken","fetchAsset","updBroken","broken","updateAsset","map","fetchAssets","getAssets","created"],"sources":["/Users/agusmanriyadi/Documents/LATIHAN/VUE 3/vuepractice/src/views/Home.vue"],"sourcesContent":["<template>\n    \n    <AssetForm v-show=\"showAdd\" @action-add=\"addAsset\"/>\n\n    <div class=\"my-2\">\n        <AssetList :assets=\"assets\" @action-delete=\"deleteAsset\" @action-broken=\"brokenAsset\"/>\n    </div>\n\n</template>\n\n<script>\nimport AssetForm from '../components/modules/form/AssetForm.vue'\nimport AssetList from '../components/modules/list/asset/AssetList.vue'\nimport {UseAssetStore} from '../store/AssetStore'\nimport axios from \"axios\"\n\nexport default {\n    name: \"Home\",\n    props:{\n        showAdd: Boolean\n    },\n    components: {\n        AssetForm,\n        AssetList,\n        UseAssetStore\n    },\n    data(){\n        return {\n            assets : [],\n            store : UseAssetStore()\n        }\n    },\n    methods:{\n        async deleteAsset(id){\n            if(confirm(\"Are you sure ?\")){\n                const res = await this.store.deleteAsset(id)\n\n                res.status === 200 ? (this.assets = this.assets.filter((asset) => asset.id !== id)) : alert(\"Error deleting asset\")\n            }\n        },\n        async addAsset(newAsset){\n            const res = await this.store.addAsset(newAsset)\n\n            if(res.status === 201){\n                alert(\"Data asset has been submit\")\n                this.assets = [...this.assets, res.data]\n            }\n\n        },\n        async brokenAsset(id){\n\n            const filterBroken = await this.fetchAsset(id)\n            const updBroken = {...filterBroken, broken: !filterBroken.broken}\n\n            const res = this.res.updateAsset(id, updBroken)\n            const data = res.data\n\n            this.assets = this.assets.map( (asset) => asset.id === id ? {...asset, broken : data.broken} : asset)\n        },\n        async fetchAssets(){\n            await this.store.fetchAssets()\n            return this.store.getAssets \n        },\n        async fetchAsset(id){\n            const data = await this.store.fetchAsset(id)\n            return data\n        }\n    },\n    async created(){\n        this.assets = await this.fetchAssets()\n    }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAWA,OAAOA,SAAQ,MAAO,0CAAyC;AAC/D,OAAOC,SAAQ,MAAO,gDAA+C;AACrE,SAAQC,aAAa,QAAO,qBAAoB;AAChD,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,KAAK,EAAC;IACFC,OAAO,EAAEC;EACb,CAAC;EACDC,UAAU,EAAE;IACRR,SAAS;IACTC,SAAS;IACTC;EACJ,CAAC;EACDO,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,MAAK,EAAI,EAAE;MACXC,KAAI,EAAIT,aAAa,CAAC;IAC1B;EACJ,CAAC;EACDU,OAAO,EAAC;IACJ,MAAMC,WAAWA,CAACC,EAAE,EAAC;MACjB,IAAGC,OAAO,CAAC,gBAAgB,CAAC,EAAC;QACzB,MAAMC,GAAE,GAAI,MAAM,IAAI,CAACL,KAAK,CAACE,WAAW,CAACC,EAAE;QAE3CE,GAAG,CAACC,MAAK,KAAM,GAAE,GAAK,IAAI,CAACP,MAAK,GAAI,IAAI,CAACA,MAAM,CAACQ,MAAM,CAAEC,KAAK,IAAKA,KAAK,CAACL,EAAC,KAAMA,EAAE,CAAC,GAAIM,KAAK,CAAC,sBAAsB;MACtH;IACJ,CAAC;IACD,MAAMC,QAAQA,CAACC,QAAQ,EAAC;MACpB,MAAMN,GAAE,GAAI,MAAM,IAAI,CAACL,KAAK,CAACU,QAAQ,CAACC,QAAQ;MAE9C,IAAGN,GAAG,CAACC,MAAK,KAAM,GAAG,EAAC;QAClBG,KAAK,CAAC,4BAA4B;QAClC,IAAI,CAACV,MAAK,GAAI,CAAC,GAAG,IAAI,CAACA,MAAM,EAAEM,GAAG,CAACP,IAAI;MAC3C;IAEJ,CAAC;IACD,MAAMc,WAAWA,CAACT,EAAE,EAAC;MAEjB,MAAMU,YAAW,GAAI,MAAM,IAAI,CAACC,UAAU,CAACX,EAAE;MAC7C,MAAMY,SAAQ,GAAI;QAAC,GAAGF,YAAY;QAAEG,MAAM,EAAE,CAACH,YAAY,CAACG;MAAM;MAEhE,MAAMX,GAAE,GAAI,IAAI,CAACA,GAAG,CAACY,WAAW,CAACd,EAAE,EAAEY,SAAS;MAC9C,MAAMjB,IAAG,GAAIO,GAAG,CAACP,IAAG;MAEpB,IAAI,CAACC,MAAK,GAAI,IAAI,CAACA,MAAM,CAACmB,GAAG,CAAGV,KAAK,IAAKA,KAAK,CAACL,EAAC,KAAMA,EAAC,GAAI;QAAC,GAAGK,KAAK;QAAEQ,MAAK,EAAIlB,IAAI,CAACkB;MAAM,IAAIR,KAAK;IACxG,CAAC;IACD,MAAMW,WAAWA,CAAA,EAAE;MACf,MAAM,IAAI,CAACnB,KAAK,CAACmB,WAAW,CAAC;MAC7B,OAAO,IAAI,CAACnB,KAAK,CAACoB,SAAQ;IAC9B,CAAC;IACD,MAAMN,UAAUA,CAACX,EAAE,EAAC;MAChB,MAAML,IAAG,GAAI,MAAM,IAAI,CAACE,KAAK,CAACc,UAAU,CAACX,EAAE;MAC3C,OAAOL,IAAG;IACd;EACJ,CAAC;EACD,MAAMuB,OAAOA,CAAA,EAAE;IACX,IAAI,CAAC